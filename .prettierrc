{
     "arrowParens": "always",
     "bracketSameLine": false,
     "bracketSpacing": true,
     "embeddedLanguageFormatting": "auto",
     "htmlWhitespaceSensitivity": "css",
     "insertPragma": false,
     "jsxSingleQuote": false,
     "tsxSingleQuote": false,
     "printWidth": 100,
     "proseWrap": "preserve",
     "quoteProps": "as-needed",
     "requirePragma": false,
     "semi": true,
     "singleQuote": true,
     "tabWidth": 5,
     "trailingComma": "all",
     "useTabs": false,
     "vueIndentScriptAndStyle": false
}
